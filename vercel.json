{
  "version": 2,
  "builds": [
    { "src": "frontend/**", "use": "@vercel/static" },
    { "src": "app/api/vercel_entry.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "app/api/vercel_entry.py" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ],
  "env": {
    "DEEPSEEK_API_KEY": "@deepseek_api_key"
  }
} 